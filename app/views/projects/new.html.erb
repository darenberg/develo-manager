 <h2 class="text-center mb-5">create a new project </h2>
<div class="p-3 m-3 shadow p-3 mb-5 bg-white rounded" >
  <%= simple_form_for(@project) do |f| %>
    <%= f.input :title %>
    <%= simple_fields_for :project_users do |p| %>
      <%= select_tag 'pproject_users',
       options_from_collection_for_select(User.all, :id, :email),
       multiple: true, class: 'multiselectuser' %>
    <% end %>
    <%= f.submit  %>
  <% end %>
</div>
