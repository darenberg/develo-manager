<div class="d-flex row">
  <div class="d-flex col m-t-15px">
  <h4>Floor Plans</h4>
  </div>
  <div class="d-flex col">
    <%@project.floors.first.plans.each do |plan| %>
      <button type="button" class="plan-btn" data-action="click->maps#changePlan" id="plan-<%= plan.stage %>"> <%= plan.stage.capitalize %> </button>
    <% end %>
  </div>
</div>
